{
  "name": "@shipfox/swc",
  "version": "1.2.1",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@10.22.0",
  "type": "module",
  "bin": {
    "swc": "./bin/swc.js"
  },
  "scripts": {
    "build": "rm -rf dist && swc --strip-leading-paths --config-file .swcrc -d dist . && tsc-alias -p tsconfig.build.json --resolve-full-paths"
  },
  "dependencies": {
    "@shipfox/tool-utils": "workspace:*",
    "@swc/cli": "^0.7.0",
    "@swc/core": "^1.11.24",
    "date-fns": "^4.1.0",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@shipfox/ts-config": "workspace:*"
  }
}
